<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="consoleLog" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern><![CDATA[date:%d{yyyy-MM-dd HH:mm:ss.SSS} level:%-5level thread:%thread class:%class method:%method %msg%n]]></pattern>
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="consoleLog" />
    </root>
</configuration>
